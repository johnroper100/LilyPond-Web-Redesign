@tab nuances
@tab
@lilypond[relative=2]
\set Staff.initialTimeSignatureVisibility = #all-invisible
c2\mf c\sfz
@end lilypond


@item @code{a\< a a\!}
@tab crescendo
@tab
@lilypond[relative=2]
\set Score.timing = ##f
